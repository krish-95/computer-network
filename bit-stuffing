#include "stdio.h"
#include "conio.h"
int main()
{
int i=0,count=0, length;
char a[100];
printf("enter the bits : ");
scanf("%s",a);
 length=strlen(a);

for(i=0;i<length; i++)
{
    if(a[i]=='1')
	count++;
    else
	count=0;
printf("%c",a[i]);
if(count==5)
    {
	printf("0");
	count=0;
    }
}

return 1;
}
